{
    "name": "TargetSweeper 360 Web",
    "short_name": "TargetSweeper360",
    "description": "KML Viewer with accurate GPS positioning for tactical and mapping applications",
    "start_url": "/",
    "display": "standalone",
    "display_override": [
        "window-controls-overlay",
        "standalone"
    ],
    "background_color": "#ffffff",
    "theme_color": "#4F46E5",
    "orientation": "any",
    "scope": "/",
    "lang": "en",
    "categories": [
        "navigation",
        "utilities",
        "productivity"
    ],
    "screenshots": [
        {
            "src": "/screenshot-desktop.png",
            "sizes": "1280x720",
            "type": "image/png",
            "form_factor": "wide"
        },
        {
            "src": "/screenshot-mobile.png",
            "sizes": "640x1136",
            "type": "image/png",
            "form_factor": "narrow"
        }
    ],
    "icons": [
        {
            "src": "/icon-192.png",
            "sizes": "192x192",
            "type": "image/png",
            "purpose": "any"
        },
        {
            "src": "/icon-512.png",
            "sizes": "512x512",
            "type": "image/png",
            "purpose": "any"
        },
        {
            "src": "/icon-maskable-192.png",
            "sizes": "192x192",
            "type": "image/png",
            "purpose": "maskable"
        },
        {
            "src": "/icon-maskable-512.png",
            "sizes": "512x512",
            "type": "image/png",
            "purpose": "maskable"
        }
    ],
    "shortcuts": [
        {
            "name": "Load KML",
            "short_name": "Load KML",
            "description": "Quickly load a KML file",
            "url": "/?action=load-kml",
            "icons": [
                {
                    "src": "/shortcut-load.png",
                    "sizes": "96x96"
                }
            ]
        },
        {
            "name": "My Location",
            "short_name": "Location",
            "description": "Center map on current location",
            "url": "/?action=location",
            "icons": [
                {
                    "src": "/shortcut-location.png",
                    "sizes": "96x96"
                }
            ]
        }
    ],
    "file_handlers": [
        {
            "action": "/",
            "accept": {
                "application/vnd.google-earth.kml+xml": [
                    ".kml"
                ],
                "application/vnd.google-earth.kmz": [
                    ".kmz"
                ]
            }
        }
    ],
    "protocol_handlers": [
        {
            "protocol": "geo",
            "url": "/?geo=%s"
        }
    ],
    "permissions": [
        "geolocation"
    ],
    "features": [
        "geolocation"
    ],
    "prefer_related_applications": false,
    "edge_side_panel": {
        "preferred_width": 400
    }
}